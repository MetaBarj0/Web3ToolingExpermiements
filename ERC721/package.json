{
  "name": "ERC721",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "chai-as-promised": "^8.0.1",
    "hardhat": "^2.22.18",
    "hardhat-watcher": "^2.5.0"
  },
  "scripts": {
    "test": "npx hardhat test",
    "compile": "npx hardhat compile",
